<MudDialog>
    <DialogContent>
        Are you sure you want to remove this Review?
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Filled"
                   Size="Size.Small"
                   OnClick="Cancel">Cancel</MudButton>
        <MudButton Variant="Variant.Filled"
                   Size="Size.Small"
                   Color="Color.Primary"
                   OnClick="Submit">Yes</MudButton>
    </DialogActions>
</MudDialog>
